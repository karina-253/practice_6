N = int(input())

#создание пустого списка
nums = []

for x in range (0, 10):
    nums.append (x)

for y in range (10, 100):
    nums.append (y // 10)
    nums.append (y % 10)

for z in range (100, 201):
    nums.append ((z // 10) // 10)
    nums.append ((z // 10) % 10)
    nums.append (z % 10)

print(nums[N-1])
